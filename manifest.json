{
  "manifest_version": 2,
  "name": "VS Code Extension Ranker",
  "description": "Instant visibility into which Visual Studio Code Extensions are most popular.",
  "icons": {
    "128": "vsc128.png"
  },
  
  "version": "1.0.0",
  
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  
  "browser_action": {
    "default_icon": "vsc16.png",
    "default_popup": "popup.html",
    "default_title": "VS Code Ext Ranker"
  },

  "permissions": ["activeTab"]
}
